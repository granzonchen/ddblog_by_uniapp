<view><cu-custom vue-id="50c0d8d6-1" isBack="{{false}}" onVueInit="__l"></cu-custom><view class="logo"><image src="/static/logo.png" mode="aspectFill"></image></view><view class="cu-bar search"><view class="search-form round"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="搜索或输入关键字" confirm-type="search" data-event-opts="{{[['input',[['setValue',['$event']]]]]}}" onInput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['handleSearch',[1]]]]]}}" class="cu-btn bg-green shadow-blur round" onTap="__e">搜索</button></view></view><block a:if="{{resultData==null&&init}}"><view class="no-result text-gray">什么也没查到~</view></block><block a:if="{{resultData&&list.length>0}}"><view class="tip text-gray">为您找到以下结果</view></block><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{resultData}}"><scroll-view><post-card vue-id="{{'50c0d8d6-2-'+index}}" post="{{item}}" onVueInit="__l"></post-card></scroll-view></block></block><block a:if="{{resultData.IPageObj.current==resultData.IPageObj.pages&&init||resultData.IPageObj.pages==0}}"><view class="no-result text-gray">没有找到其他数据</view></block><bottom-nav vue-id="50c0d8d6-3" active="{{pageName}}" onVueInit="__l"></bottom-nav></view>